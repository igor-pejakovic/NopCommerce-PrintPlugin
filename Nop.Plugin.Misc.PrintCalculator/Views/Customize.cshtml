        inputStapling.off('click')
        $('#optStaple').hide()
        $(document).ready(() => imagePreview.attr('src', '{Model.Thumbnail}'))
        $('#thumbnail').val('{Model.Thumbnail}')
        </script>";

        Html.AddInlineScriptParts(ResourceLocation.Footer, thumbCode);
    }
    else
    {
        var thumbDocu = $@"<script>
        var imagePreviewSrc = $('#preview_img').attr('src')
        $('#thumbnail').val(imagePreviewSrc)
        </script>";

        Html.AddInlineScriptParts(ResourceLocation.Footer, thumbDocu);
    }



}
<head>
    <link rel="stylesheet" href="@Url.Content("~/Plugins/Misc.PrintCalculator/Views/Styles/Customize.css")">
    <link rel="stylesheet" href="@Url.Content("~/Plugins/Misc.PrintCalculator/Views/Styles/Spinner.css")">
</head>
<div class="overlay"></div>
<form asp-controller="Print" asp-action="Create" method="post">
    <div id="print-preview">
        <div>
            <img id="preview_img" alt="" src="data:image/png;base64," />
        </div>
        <div id="print-details">
            <strong>@T("Plugins.Misc.PrintCalculator.PrintOptions.PageCount"): @Model.PageCount</strong>
            <div class="spacer"></div>
            <div>
                <div class="option">
                    @T("Plugins.Misc.PrintCalculator.PrintOptions.PaperSize"):
                    @Html.DropDownListFor(x => Model.PaperSize, new SelectList(Model.PaperSize, "Value", "Text"), htmlAttributes: new { id = "PaperSize" })
                </div>
                <div class="option">
                    @T("Plugins.Misc.PrintCalculator.PrintOptions.PaperType"):
                    @Html.DropDownListFor(x => Model.PaperType, new SelectList(Model.PaperType, "Value", "Text"), htmlAttributes: new { id = "PaperType" })
                </div>
                <div class="spacer"></div>
                <div class="option check">
                    @T("Plugins.Misc.PrintCalculator.PrintOptions.Duplex")
                    @Html.CheckBox(nameof(Model.Duplex))
                </div>
                <div class="option check" id="optStaple">
                    @T("Plugins.Misc.PrintCalculator.PrintOptions.Stapling")
                    @Html.CheckBox(nameof(Model.Stapling))
                </div>
                <div class="option check">
                    @T("Plugins.Misc.PrintCalculator.PrintOptions.PrintColor")
                    @Html.CheckBox(nameof(Model.PrintColor))
                </div>
                <div class="spacer"></div>
                <div class="option">
                    <strong>
                        @T("Plugins.Misc.PrintCalculator.PrintOptions.Price"):
                        <span id="price">0</span> KM
                    </strong>
                </div>
                <div class="option">
                    @T("Plugins.Misc.PrintCalculator.PrintOptions.NextStep")
                </div>
                <div class="spacer"></div>
            </div>
            <input type="hidden" name="name" value="@Model.Reference" />
            <input type="hidden" id="thumbnail" name="thumbnail" />
            <input type="submit" id="submit-print" value="@T("Plugins.Misc.PrintCalculator.PrintOptions.Submit")" />
        </div> 
    </div>
</form>
