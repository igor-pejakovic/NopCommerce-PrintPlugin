@model Nop.Plugin.Misc.PrintCalculator.Models.CustomizeModel
@using Nop.Web.Framework.UI
@{
    Layout = "_ColumnsOne";
    Html.AddScriptParts(ResourceLocation.Footer, "~/Plugins/Misc.PrintCalculator/Views/Scripts/imageUpdate.js", "~/Plugins/Nop.Plugin.Misc.PrintCalculator/Scripts/imageUpdate.js", true);
    var priceOptions = Model.Options;
}

@{ 
    var collection = @Html.Raw(Json.Serialize(Model.Options));
    var loadScript = "initPrice(" + collection + ", " + Model.PageCount + ")";

    string BP = "BasePrice";

    var priceCode = $@"<script>
                    var priceSpan = $('#price')
                    var priceDict = {collection}
                    var inputStapling = $('#Stapling')
                    var inputDuplex = $('#Duplex')
                    var inputPrintColor = $('#PrintColor')
                    var inputPaperSize = $('#PaperSize')
                    var inputPaperType = $('#PaperType')
                    var basePrice = {Model.Options[BP]}
                    var pageCount = {Model.PageCount}
                    $('#submit-print').click(function() {{$('body').addClass('loading')}})

                    function updatePrice() {{
                        var price = basePrice * pageCount
                        if (inputDuplex.is(':checked')) {{
                            if(pageCount %2 == 1) {{price = basePrice * (pageCount + 1)}}
                            price *= priceDict['Duplex']
                            }}
                        var paperSizeKey = inputPaperSize.find('option:selected').val();
                        var paperTypeKey = inputPaperType.find('option:selected').val();
                        price *= priceDict[paperSizeKey]
                        price *= priceDict[paperTypeKey]
                        
                        if (inputStapling.is(':checked')) {{price += priceDict['Stapling']}}
                        if (inputPrintColor.is(':checked')) {{price *= priceDict['PrintColor']}}
                        priceSpan.text(price.toFixed(2))
                    }}
                    $(document).ready(updatePrice)
                    inputStapling.click(updatePrice)
                    inputDuplex.click(updatePrice)
                    inputPrintColor.click(updatePrice)    
                    inputPaperSize.click(updatePrice)
                    inputPaperType.click(updatePrice)
    </script>
    ";
    Html.AddInlineScriptParts(ResourceLocation.Footer, priceCode);

    if (!String.IsNullOrEmpty(Model.Thumbnail))
    {
        var thumbCode = $@"<script> 
        var imagePreview = $('#preview_img')
